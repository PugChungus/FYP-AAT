<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css">
    <link rel="stylesheet" type="text/css" href="/css/home.css">
    <link rel="stylesheet" type="text/css" href="/css/loading.css">
    <link rel="stylesheet" type="text/css" href="/css/navbar.css">
    <link rel="stylesheet" type="text/css" href="/css/keymanagement.css">


    <title>Key Management</title>
</head>


<body id="body-pd" onload="createKeyDropdown()">
    <header class="header" id="header">
        <div class="header_toggle"> <i class='bx bx-menu' id="header-toggle"></i> </div>
        <div class="header_img" id="profile-pic"> 
            <img id="profile-picture" src="../images/account_circle_FILL0_wght200_GRAD200_opsz24.png" alt="Profile Picture">
        </div>
    </header>
    <div class="l-navbar" id="nav-bar">
        <nav class="nav">
            <div>
                <a href="/home" class="nav_logo"> <i class='bx bx-layer nav_logo-icon'></i> <span class="nav_logo-name">JTR</span> </a>
                <div class="nav_list">
                    <a href="/home" class="nav_link"> <i class='bx bx-home' ></i> <span class="nav_name">Home</span> </a>
                    <a href="/encrypt" class="nav_link"> <i class='bx bxs-lock'></i> <span class="nav_name">Encrypt</span> </a>
                    <a href="/decrypt" class="nav_link"> <i class='bx bxs-lock-open'></i> <span class="nav_name">Decrypt</span> </a>
                    <a href="/keymanagement" class="nav_link active"> <i class='bx bx-key'></i> <span class="nav_name">Key Management</span> </a>
                    <a href="/history" class="nav_link"> <i class='bx bx-history'></i> <span class="nav_name">History</span> </a>
                    <a href="/settings" class="nav_link"> <i class='bx bxs-cog'></i> <span class="nav_name">Settings</span> </a>
                </div>
            </div>
            <i class="nav_link" id="signOutLink"> 
                <i class='bx bx-log-out nav_icon' id="signOutIcon"></i>
                <span class="nav_name" id="signOutText">Sign Out</span>
            </i>
        </nav>
    </div>
    <div id="overlay" class="overlay"></div>

    <div class="container">
        <div class="Header">
            <div class="keyHead">
                <h2>AES Key Management</h2>
            </div>
            <div class="buttons">
                <!-- <label for="fileInput" class="importKey" data-label="Import Key">
                    <i class='bx bx-import bx-sm'></i>
                </label> -->
                <a href="#" class="importKey" data-label="Import Key" id="importKey"><i class='bx bx-import bx-sm'></i></a>
                <input type="file" id="fileInput" style="display: none;">         
                <a href="#" class="generateKey" data-label="Generate Key" id="generateKeyButton"><i class='bx bx-add-to-queue bx-sm'></i></a>
            </div>
        </div>
        <ul class="responsive-table">
          <li class="table-header">
            <div class="col col-1">Id</div>
            <div class="col col-2">Key Name</div>
            <div class="col col-3">Date Created</div>
            <div class="col col-4 col-right"></div>
          </li>
        </ul>
    </div>

    <!-- keycard.html -->

    <div id="keyCard">
        <span id="keyCardClose" onclick="closeKeyCard()">&times;</span>
        <h3>Generate New Key</h3>
        <label for="keyName">Key Name:</label>
        <input type="text" id="keyName" placeholder="Enter Key Name">
        <button onclick="generateKey()">Generate Key</button>
    </div>




    <script src="/js/navbar.js"></script>
    <script src="/js/home.js"></script>
    <script src="/js/toprofilePage.js"></script>
    <script src="/js/keygen.js"></script>
    <!-- <script src="/js/callnavbar.js"></script> -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>